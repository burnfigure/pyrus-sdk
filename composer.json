{
  "name": "burnfigure/pyrus-sdk",
  "description": "Pyrus SDK PHP-client",
  "version": "1.1",
  "keywords": [
    "pyrus", "api", "sdk", "client", "php"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Vladimir Nikitin",
      "email": "burnfigure@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "ext-curl": "*",
    "guzzlehttp/guzzle": "^7.5"
  },
  "autoload": {
    "psr-4": {
      "Pyrus\\": "src",
      "Pyrus\\Library\\": "src/Library",
      "Pyrus\\Entities\\": "src/Entities",
      "Pyrus\\Abstracts\\": "src/Abstracts",
      "Pyrus\\Interfaces\\": "src/Interfaces",
      "Pyrus\\Traits\\": "src/Traits"
    }
  }
}
